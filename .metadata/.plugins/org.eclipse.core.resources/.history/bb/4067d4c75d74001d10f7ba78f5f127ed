
public class RSMain {
	public static void main(String[] args) {

		for (int i = 0; i < 5; i++) {
			System.out.println("ã…Ž");
		}
		System.out.println("---------");

		// 3~5
		for (int i = 3; i < 6; i++) {
			System.out.println(i);
		}
		System.out.println("---------");

		// 1~5
		for (int i = 1; i < 6; i++) {
			System.out.println(i);
		}

		System.out.println("----------");

		// 1 3 5 7 9
		for (int i = 1; i < 10; i += 2) {
			System.out.println(i);
		}

		System.out.println("-----------");

		// 10 ~ 1
		for (int i = 10; i > 0; --i) {
			System.out.println(i);
		}
		System.out.println("-----------");
		
		//1 ~ 10
		for (int i = 1; i < 10; i++) {
			System.out.println(i);
		}
		System.out.println("-----------");
		
		
	}
}
